{
  "name": "llmcli",
  "version": "0.1.6",
  "description": "The LLMCLI is a command-line tool that allows you to interact with the Bedrock AI model. It provides various tools and capabilities to help you accomplish any cli tasks",
  "main": "dist/index.js",
  "bin": {
    "llmcli": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "CLI",
    "LLM",
    "GenAI",
    "Productivity"
  ],
  "author": "Adler Oliveira",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.1",
    "@types/ora": "^3.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.699.0",
    "@aws-sdk/client-bedrock-runtime": "^3.699.0",
    "@aws-sdk/client-sts": "^3.699.0",
    "@aws-sdk/credential-providers": "^3.699.0",
    "@smithy/shared-ini-file-loader": "^3.1.11",
    "ansi-parser": "^3.2.10",
    "chalk": "^5.3.0",
